{
    "version": 2,
    "builds": [
        {
            "src": "server/dist/index.js",
            "use": "@vercel/node",
            "config": { "includeFiles": ["dist/**"] }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server/dist/index.js"
        }
    ]
}